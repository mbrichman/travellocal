<!DOCTYPE html>
<html>
<head>
  <title>Travellocal</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>

  <nav>
    <ul>
      <% if signed_in? %>
      <li><%= link_to 'Users', users_url %></li>
      <li><%= link_to 'Reviews', reviews_url %></li>
      <li><%= link_to 'Cities', cities_url %></li>
      <li><%= link_to 'Places', places_url %></li>
      <li><%= link_to 'Log Out', sessions_destroy_url %></li>
      <% else %>
      <li><%= link_to 'Log In', sessions_new_url %></li>
      <% end %>
    </ul>
  </nav>
</head>
<body>

<%= yield %>

</body>
</html>
